<odoo>

    <!-- ============================= -->
    <!-- Insurance Administrator -->
    <!-- ============================= -->
    <record id="group_insurance_admin" model="res.groups">
        <field name="name">Insurance Administrator</field>
    </record>

    <!-- ============================= -->
    <!-- Medical Committee -->
    <!-- ============================= -->
    <record id="group_insurance_committee" model="res.groups">
        <field name="name">Medical Committee</field>
    </record>

    <!-- ============================= -->
    <!-- Insurance Manager -->
    <!-- ============================= -->
    <record id="group_insurance_manager" model="res.groups">
        <field name="name">Insurance Manager</field>
    </record>

    <!-- ============================= -->
    <!-- General Manager -->
    <!-- ============================= -->
    <record id="group_insurance_gm" model="res.groups">
        <field name="name">General Manager</field>
    </record>

    <!-- ============================= -->
    <!-- Insurance Finance -->
    <!-- ============================= -->
    <record id="group_insurance_finance" model="res.groups">
        <field name="name">Insurance Finance</field>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]" />
    </record>

    <!-- ============================= -->
    <!-- Insurance Treasury -->
    <!-- ============================= -->
    <record id="group_insurance_treasury" model="res.groups">
        <field name="name">Insurance Treasury</field>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]" />
    </record>

</odoo>