<odoo>

    <record id="view_insurance_service_list" model="ir.ui.view">
        <field name="name">insurance.service.list</field>
        <field name="model">insurance.service</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="view_insurance_service_form" model="ir.ui.view">
        <field name="name">insurance.service.form</field>
        <field name="model">insurance.service</field>
        <field name="arch" type="xml">
            <form string="Insurance Service">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="active"/>
                    </group>
                    <field name="description"/>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_insurance_service" model="ir.actions.act_window">
        <field name="name">Services</field>
        <field name="res_model">insurance.service</field>
        <field name="view_mode">list,form</field>
    </record>

</odoo>
