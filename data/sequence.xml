<odoo>
    <record id="seq_insurance_claim" model="ir.sequence">
        <field name="name">Insurance Claim</field>
        <field name="code">insurance.claim</field>
        <field name="prefix">CLM/%(year)s/</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_reinsurance_bordereau" model="ir.sequence">
        <field name="name">Reinsurance Bordereau</field>
        <field name="code">insurance.reinsurance.bordereau</field>
        <field name="prefix">RB/%(year)s/</field>
        <field name="padding">5</field>
    </record>
    <record id="seq_reinsurance_settlement" model="ir.sequence">
        <field name="name">Reinsurance Settlement</field>
        <field name="code">insurance.reinsurance.settlement</field>
        <field name="prefix">RS/%(year)s/</field>
        <field name="padding">4</field>
    </record>

</odoo>
